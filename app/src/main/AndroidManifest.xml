<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wehand">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.MainActivity" />
        <activity android:name=".ui.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.NewUserActivity" />
        <activity android:name=".ui.HightPressTipsActivity" />
        <activity android:name=".ui.HightSugarTipsActivity" />
        <activity android:name=".ui.AboutUsActivity" />
        <activity android:name=".ui.UserRequstionActivity" />
        <activity android:name=".ui.LangSetActivity" />
        <activity android:name=".ui.UserDetailActivity"></activity>
    </application>

</manifest>